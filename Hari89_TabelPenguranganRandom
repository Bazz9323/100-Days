import java.util.Random;

public class Hari89_TabelPenguranganRandom {
    public static void main(String[] args) {
        Random random = new Random();
        int angka = random.nextInt(10);
        for (int i = 1; i <= 10; i++) {
            System.out.println(angka + " - "+ i + " = " + (angka-i));
        }
    }
}
