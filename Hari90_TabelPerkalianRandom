import java.util.Random;

public class Hari90_TabelPerkalianRandom {
    public static void main(String[] args) {
        Random random = new Random();
        int angka = random.nextInt(10);
        for (int i = 1; i <= 10; i++) {
            System.out.println(angka + " x "+ i + " = " + (angka*i));
        }
    }
}
