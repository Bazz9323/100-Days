import java.util.Random;

public class Hari91_TabelPembagianRandom {
    public static void main(String[] args) {
        Random random = new Random();
        double angka = random.nextDouble(10);
        for (int i = 1; i <= 10; i++) {
            System.out.println(angka + " / "+ i + " = " + (angka/i));
        }
    }
}
